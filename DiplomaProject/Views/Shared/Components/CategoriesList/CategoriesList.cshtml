@model IEnumerable<DiplomaProject.Models.Category>

<h4>Категорії товарів</h4>
<div class="list-group list-group-horizontal-md">
    @foreach (var item in Model)
    {
        <a asp-action="Index" asp-controller="Product" asp-route-categoryId="@item.Id" class="list-group-item list-group-item-action">@item.Name</a>
    }
</div>